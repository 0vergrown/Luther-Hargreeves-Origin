{
    "type":"origins:multiple",
    "on_ground":{
       "type":"origins:resource",
       "min":0,
       "max":1,
       "hud_render":{
          "should_render":false
       },
       "start_value":0,
       "max_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:play_sound",
                "sound":"minecraft:entity.generic.explode",
                "volume":0.70,
                "pitch":1.00
             },
             {
                "type":"origins:apply_effect",
                "effect":{
                   "effect":"minecraft:slowness",
                   "duration":60,
                   "amplifier":0,
                   "is_ambient":true,
                   "show_particles":false,
                   "show_icon":false
                }
             },
             {
                "type":"origins:area_of_effect",
                "radius":4,
                "shape":"cube",
                "bientity_action":{
                   "type":"origins:and",
                   "actions":[
                      {
                         "type":"origins:add_velocity",
                         "x":0.00,
                         "y":0.80,
                         "z":0.50,
                         "reference":"position",
                         "client":true,
                         "server":true,
                         "set":false
                      },
                      {
                         "type":"origins:target_action",
                         "action":{
                            "type":"origins:damage",
                            "amount":8,
                            "damage_type":"minecraft:fall"
                         }
                      }
                   ]
                },
                "include_actor":false
             },
             {
                "type":"origins:damage",
                "amount":11,
                "damage_type":"minecraft:fall"
             },
             {
                "type":"origins:delay",
                "action":{
                   "type":"origins:passenger_action",
                   "action":{
                      "type":"origins:change_resource",
                      "resource":"luther_hargreeves:grab_if_on_ground",
                      "change":1,
                      "operation":"add"
                   },
                   "recursive":true
                },
                "ticks":2
             },
             {
                "type":"origins:spawn_particles",
                "particle":"minecraft:explosion",
                "count":5,
                "speed":0.50,
                "force":false,
                "spread":{
                   "x":0.50,
                   "y":0.50,
                   "z":0.50
                },
                "offset_y":0.5
             },
             {
                "type":"origins:spawn_particles",
                "particle":"minecraft:cloud",
                "count":10,
                "speed":0.1,
                "force":false,
                "spread":{
                   "x":1.50,
                   "y":1.50,
                   "z":1.50
                },
                "offset_y":0.5
             },
             {
                "type":"origins:delay",
                "action":{
                   "type":"origins:revoke_power",
                   "power":"luther_hargreeves:grab_target",
                   "source":"luther_hargreeves:grab_target"
                },
                "ticks":5
             }
          ]
       }
    },
    "clock":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:change_resource",
          "resource":"*:*_on_ground",
          "change":1,
          "operation":"set"
       },
       "interval":1,
       "condition":{
          "type":"origins:on_block"
       }
    },
    "fall":{
       "type":"origins:modify_falling",
       "velocity":0.16,
       "take_fall_damage":false,
       "condition":{
          "type":"origins:resource",
          "resource":"*:*_on_ground",
          "comparison":"==",
          "compare_to":0
       }
    }
}